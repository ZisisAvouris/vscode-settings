{
    "workbench.colorCustomizations": {
        "editor.background": "#0e1a27",
        "window.activeBorder": "#0e1a27",
        "titleBar.activeBackground": "#0e1a27",
        "titleBar.inactiveBackground": "#0e1a27",
        "activityBar.background": "#0e1a27",
        "sideBar.background": "#0e1a27",
        "tab.activeBackground": "#0e1a27",
        "tab.inactiveBackground": "#0e1a27",
        "statusBar.background": "#0e1a27",
        "panel.background": "#0e1a27",
        "sideBarSectionHeader.background": "#0e1a27",
        "editorGroupHeader.noTabsBackground": "#0e1a27",
        "editorGroupHeader.tabsBackground": "#0e1a27",
        "tab.hoverBackground": "#0e1a27",
    },
    "editor.tokenColorCustomizations": {
        "[Default Dark Modern]": {
            "textMateRules": [
                {
                    "scope": "variable.other.enummember.cpp",
                    "settings": {
                        "foreground": "#b6cdb0"
                    }
                },
                {
                    "scope": "entity.name.function.preprocessor.cpp",
                    "settings": {
                        "foreground": "#bfb7fe",
                    }
                },
                {
                    "scope": "variable.other.property.cpp",
                    "settings": {
                        "foreground": "#d4daec",
                    }
                },
                {
                    "scope": "entity.name.function",
                    "settings": {
                        "foreground": "#d1ce9e",
                    }
                },
                {
                    "scope": "comment",
                    "settings": {
                        "foreground": "#576370",
                        "fontStyle": "italic"
                    }
                },
                {
                    "scope": ["entity.name.type.cpp", "entity.name.type.class.templated.cpp", "entity.name.namespace.cpp"],
                    "settings": {
                        "foreground": "#5eaa9b",
                    }
                },
                {
                    "scope": "variable",
                    "settings": {
                        "foreground": "#afddf5",
                    }
                },
                {
                    "scope": ["constant.numeric", "string", "keyword.other.unit.hexadecimal.cpp"],
                    "settings": {
                        "foreground": "#b68e82",
                    }
                }
            ]
        }
    }
}